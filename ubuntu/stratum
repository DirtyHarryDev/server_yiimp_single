startstop_service() {
    cmd=$1
    name=$2
    sudo service $name $cmd
}

startstop_sha256() {
    cmd=$1
    case $cmd in
        stop) $sha256stop ;;
        start) $sha256 ;;
        restart)
            $sha256stop
            sleep 1
            $sha256
            ;;
    esac
}

startstop_sha256t() {
