case "$1" in
    start|stop|restart) cmd=$1 ;;
    *)
        shift
        servicenames=${@-servicenames}
        echo "usage: $0 [start|stop|restart] algo"
        exit 1
esac
shift

for name; do
    case "$name" in
    sha256) startstop_sha256 $cmd ;;
    sha256t) startstop_sha256t $cmd ;;
    lbry) startstop_lbry $cmd ;;
    scrypt) startstop_scrypt $cmd ;;
    timetravel) startstop_timetravel $cmd ;;
    bitcore) startstop_bitcore $cmd ;;
    exosis) startstop_exosis $cmd ;;
    c11) startstop_c11 $cmd ;;
    deep) startstop_deep $cmd ;;
    x11) startstop_x11 $cmd ;;
    x11evo) startstop_x11evo $cmd ;;
    x12) startstop_x12 $cmd ;;
    x13) startstop_x13 $cmd ;;
    x15) startstop_x15 $cmd ;;
    x16r) startstop_x16r $cmd ;;
    x16s) startstop_x16s $cmd ;;
    x17) startstop_x17 $cmd ;;
    x22i) startstop_x22i $cmd ;;
    aergo) startstop_aergo $cmd ;;
    xevan) startstop_xevan $cmd ;;
    hmq1725) startstop_hmq1725 $cmd ;;
    nist5) startstop_nist5 $cmd ;;
    x14) startstop_x14 $cmd ;;
    quark) startstop_quark $cmd ;;
    whirlpool) startstop_whirlpool $cmd ;;
    neoscrypt) startstop_neoscrypt $cmd ;;
    argon2) startstop_argon2 $cmd ;;
    argon2ddyn) startstop_argon2ddyn $cmd ;;
    scryptn) startstop_scryptn $cmd ;;
    allium) startstop_allium $cmd ;;
    lbk3) startstop_lbk3 $cmd ;;
    lyra2) startstop_lyra2 $cmd ;;
    lyra2v2) startstop_lyra2v2 $cmd ;;
    lyra2z) startstop_lyra2z $cmd ;;
    jha) startstop_jha $cmd ;;
    qubit) startstop_qubit $cmd ;;
    zr5) startstop_zr5 $cmd ;;
    skein) startstop_skein $cmd ;;
    sib) startstop_sib $cmd ;;
    keccak) startstop_keccak $cmd ;;
    keccakc) startstop_keccakc $cmd ;;
    hex) startstop_hex $cmd ;;
    skein2) startstop_skein2 $cmd ;;
    dmdgr) startstop_dmdgr $cmd ;;
    myrgr) startstop_myrgr $cmd ;;
    zr5) startstop_zr5 $cmd ;;
    blake) startstop_blake $cmd ;;
    blakecoin) startstop_blakecoin $cmd ;;
    decred) startstop_decred $cmd ;;
    vanilla) startstop_vanilla $cmd ;;
    blake2s) startstop_blake2s $cmd ;;
    penta) startstop_penta $cmd ;;
    rainforest) startstop_rainforest $cmd ;;
    luffa) startstop_luffa $cmd ;;
    m7m) startstop_m7m $cmd ;;
    veltor) startstop_veltor $cmd ;;
    velvet) startstop_velvet $cmd ;;
    vitalium) startstop_vitalium $cmd ;;
    yescrypt) startstop_yescrypt $cmd ;;
    yescryptR16) startstop_yescryptR16 $cmd ;;
    yescryptR32) startstop_yescryptR32 $cmd ;;
    bastion) startstop_bastion $cmd ;;
    hsr) startstop_hsr $cmd ;;
    phi) startstop_phi $cmd ;;
    phi2) startstop_phi2 $cmd ;;
    polytimos) startstop_polytimos $cmd ;;
    skunk) startstop_skunk $cmd ;;
    tribus) startstop_tribus $cmd ;;
    a5a) startstop_a5a $cmd ;;
    x16rv2) startstop_x16rv2 $cmd ;;
    curvehash) startstop_curvehash $cmd ;;
    x11k) startstop_x11k $cmd ;;
    megabtx) startstop_megabtx $cmd ;;
    megamec) startstop_megamec $cmd ;;
        *) startstop_service $cmd $name ;;
    esac
done
